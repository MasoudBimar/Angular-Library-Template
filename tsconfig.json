{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "angular-project": [
        "dist/angular-project"
      ],
      "angular-project/*": [
        "dist/angular-project/*"
      ],
      "core-js/es7/reflect": [
        "node_modules/core-js/proposals/reflect-metadata"
      ],
      "core-js/es6/*": [
        "node_modules/core-js/es/*"
      ],
      "app-core": [
        "dist/app-core"
      ],
      "app-core/*": [
        "dist/app-core/*"
      ],
      "app-security": [
        "dist/app-security"
      ],
      "app-security/*": [
        "dist/app-security/*"
      ]
    }
  }
}